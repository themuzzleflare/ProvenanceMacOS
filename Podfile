install! 'cocoapods', :warn_for_unused_master_specs_repo => false

platform :osx, '11.0'
inhibit_all_warnings!

target 'Provenance' do
  use_frameworks!
  
  pod 'Alamofire'
  pod 'SwiftDate'
  pod 'SnapKit'
  pod 'BonMot'

end

target 'Provenance Intents' do
  use_frameworks!
  
  pod 'Alamofire'
  pod 'SwiftDate'

end

target 'Provenance Widgets' do
  use_frameworks!
  
  pod 'Alamofire'
  pod 'SwiftDate'

end
